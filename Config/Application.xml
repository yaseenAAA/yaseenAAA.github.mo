<ViewerApplication LogoFilePath="Images/logo.png" LayoutFilePath="Config/Layouts/Wings.xaml">
  <TitleText>ArcGIS Viewer for Silverlight</TitleText>
  <Version>3.3.0.1</Version>
  <HelpLinks>
    <HelpLink>
      <Url>http://links.esri.com/silverlightviewer/help</Url>
      <DisplayText>Help</DisplayText>
    </HelpLink>
    <HelpLink>
      <Url>http://forums.arcgis.com/forums/211-ArcGIS-Viewer-for-Silverlight</Url>
      <DisplayText>Forums</DisplayText>
    </HelpLink>
    <HelpLink>
      <Url>http://blogs.esri.com/Dev/blogs/silverlightwpf/</Url>
      <DisplayText>Blog</DisplayText>
    </HelpLink>
  </HelpLinks>
  <Extensions>
    <Extension Url="Extensions/SearchTool.AddIns.xap" />
    <Extension Url="Extensions/QueryTool.AddIns.xap" />
    <Extension Url="Extensions/QueryRelatedRecords.AddIns.xap" />
    <Extension Url="Extensions/Esri.ArcGIS.Client.Application.AddIns.ExtentQueryStringBehavior.xap" />
    <Extension Url="Extensions/Esri.ArcGIS.Client.Application.AddIns.CenterQueryStringBehavior.xap" />
    <Extension Url="Extensions/Esri.ArcGIS.Client.Application.AddIns.WebMapQueryStringBehavior.xap" />
    <Extension Url="Extensions/Esri.ArcGIS.Client.Application.AddIns.ScaleQueryStringBehavior.xap" />
    <Extension Url="Extensions/Esri.ArcGIS.Client.Application.AddIns.ScaleLevelQueryStringBehavior.xap" />
  </Extensions>
  <ArcGISOnlineSharing>http://www.arcgis.com/sharing/</ArcGISOnlineSharing>
  <ArcGISOnlineSharingSecure>https://www.arcgis.com/sharing/</ArcGISOnlineSharingSecure>
  <ArcGISOnlineProxy></ArcGISOnlineProxy>
  <GeometryService>http://tasks.arcgisonline.com/ArcGIS/rest/services/Geometry/GeometryServer</GeometryService>
  <Proxy></Proxy>
  <BingMapsAppId></BingMapsAppId>
  <PortalAppId></PortalAppId>
  <WebMap>
    <ID></ID>
    <Title></Title>
    <Linked></Linked>
  </WebMap>
</ViewerApplication>